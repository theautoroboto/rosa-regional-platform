# =============================================================================
# Maestro Agent IoT Provisioning - Example Configuration
# =============================================================================
# This file shows example values. The actual terraform.tfvars is generated
# by the provision-maestro-agent-iot.sh script by parsing values from the
# management cluster's terraform.tfvars file.
#
# DO NOT commit terraform.tfvars to git - it may contain sensitive information
# =============================================================================

# Management Cluster Configuration
management_cluster_id = "management-01"

# AWS Configuration
region_name = "us-east-1"

# MQTT Configuration (must match regional cluster)
mqtt_topic_prefix = "sources/maestro/consumers"

# Tagging (copied from management cluster tfvars)
app_code      = "APP001"
service_phase = "development"
cost_center   = "123"

# Additional tags (optional)
tags = {
  Environment = "dev"
  Team        = "platform"
}
